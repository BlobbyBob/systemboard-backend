{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "default": {},
  "additionalProperties": true,
  "required": [
    "rating"
  ],
  "properties": {
    "rating": {
      "$id": "#/properties/rating",
      "type": "integer",
      "minimum": 1,
      "maximum": 5,
      "title": "rating",
      "description": "Rate the boulder",
      "examples": [
        4
      ]
    },
    "grade": {
      "$id": "#/properties/grade",
      "type": "integer",
      "minimum": 8,
      "maximum": 24,
      "title": "grade",
      "description": "Grade the boulder",
      "examples": [
        12
      ]
    }
  }
}